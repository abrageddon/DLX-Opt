main
	var i, j;
	var element;
	array[5][6] arr;
{
	let i <- 0;
	while i < 5 do
		let j <- 0;
		while j < 6 do 
			let arr[i][j] <- i + j;
			let element <- arr[i][j];
			let j <- j + 1;
		od; 		
		let i <- i + 1;
	od;
	
	let i <- 0;
	while i < 5 do
		let j <- 0;
		while j < 6 do 
			let element <- arr[i][j];
			call OutputNum(element);
			let j <- j + 1;
		od; 		
		let i <- i + 1;
		call OutputNewLine();
	od;
}.
